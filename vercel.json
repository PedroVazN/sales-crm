{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "buildCommand": "echo 'No build needed for Node.js backend'",
  "outputDirectory": ".",
  "installCommand": "npm install"
}