{
  "version": 2,
  "builds": [
    {
      "src": "complete-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "complete-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ],
  "functions": {
    "complete-server.js": {
      "maxDuration": 30
    }
  }
}
